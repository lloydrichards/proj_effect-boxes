{
  "name": "effect-boxes",
  "version": "0.1.0",
  "description": "A functional TypeScript library for creating and manipulating text-based box layouts using Effect.js patterns",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./Box": {
      "types": "./src/Box.ts",
      "import": "./src/Box.ts"
    },
    "./Annotation": {
      "types": "./src/Annotation.ts",
      "import": "./src/Annotation.ts"
    },
    "./Ansi": {
      "types": "./src/Ansi.ts",
      "import": "./src/Ansi.ts"
    },
    "./Cmd": {
      "types": "./src/Cmd.ts",
      "import": "./src/Cmd.ts"
    },
    "./Reactive": {
      "types": "./src/Reactive.ts",
      "import": "./src/Reactive.ts"
    }
  },
  "files": [
    "src/**/*.ts"
  ],
  "keywords": [
    "effect",
    "typescript",
    "functional",
    "box-layout",
    "terminal",
    "ansi",
    "text-rendering"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lloydrichards/proj_effect-boxes.git"
  },
  "homepage": "https://github.com/lloydrichards/proj_effect-boxes#readme",
  "bugs": {
    "url": "https://github.com/lloydrichards/proj_effect-boxes/issues"
  },
  "scripts": {
    "start": "bun run src/main.ts",
    "lint": "biome lint",
    "type-check": "tsc --noEmit",
    "format": "biome check --write",
    "test": "vitest run",
    "test:effect": "vitest run tests/box-effect.test.ts",
    "test:watch": "vitest",
    "scratch": "bun run scratchpad/index.ts",
    "benchmark": "vitest bench --run",
    "benchmark:quick": "vitest bench benchmarks/quick.bench.ts --run"
  },
  "dependencies": {
    "effect": "^3.17.14"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@effect/cli": "^0.70.0",
    "@effect/language-service": "^0.41.0",
    "@effect/platform": "^0.91.1",
    "@effect/platform-bun": "^0.80.0",
    "@effect/vitest": "^0.26.0",
    "@types/bun": "^1.2.23",
    "@vitest/coverage-v8": "3.2.4",
    "tinybench": "^5.0.1",
    "typescript": "^5.9.2",
    "ultracite": "5.4.5",
    "vitest": "^3.2.4"
  }
}
